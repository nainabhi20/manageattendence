<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.subject_list_view_in_add_subject">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="403dp"
        android:layout_height="93dp"
        android:background="@drawable/home_fragment_constract_layout_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499">

        <TextView
            android:id="@+id/subject_name_list_view_add_subject"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-condensed-light"
            android:text="TextView"
            android:textColor="#FFF8F8"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.067" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-condensed-light"
            android:text="Presents:"
            android:textColor="#F6F2F2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/subject_name_list_view_add_subject"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/subject_name_list_view_add_subject"
            app:layout_constraintTop_toBottomOf="@+id/subject_name_list_view_add_subject"
            app:layout_constraintVertical_bias="0.52" />

        <TextView
            android:id="@+id/present_list_view_add_subject"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-condensed-light"
            android:text="100"
            android:textColor="#F3EEEE"
            app:layout_constraintBottom_toBottomOf="@+id/textView20"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/textView20"
            app:layout_constraintTop_toTopOf="@+id/textView20"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-condensed-light"
            android:text="Total classes:"
            android:textColor="#F3F2F2"
            app:layout_constraintBottom_toBottomOf="@+id/present_list_view_add_subject"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.032"
            app:layout_constraintStart_toEndOf="@+id/present_list_view_add_subject"
            app:layout_constraintTop_toTopOf="@+id/present_list_view_add_subject"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/total_classes_list_view_add_subject"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-condensed-light"
            android:text="1000"
            android:textColor="#FAFAFA"
            app:layout_constraintBottom_toBottomOf="@+id/textView23"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/textView23"
            app:layout_constraintTop_toTopOf="@+id/textView23"
            app:layout_constraintVertical_bias="0.0" />

        <ImageView
            android:id="@+id/delete_subject_list_view_add_subject"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/total_classes_list_view_add_subject"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.905"
            app:layout_constraintStart_toEndOf="@+id/total_classes_list_view_add_subject"
            app:layout_constraintTop_toTopOf="@+id/total_classes_list_view_add_subject"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/ic_baseline_delete_outline_24" />

        <ImageView
            android:id="@+id/reset_attendence_list_view_add_attendence"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/delete_subject_list_view_add_subject"
            app:layout_constraintEnd_toStartOf="@+id/delete_subject_list_view_add_subject"
            app:layout_constraintHorizontal_bias="0.946"
            app:layout_constraintStart_toEndOf="@+id/total_classes_list_view_add_subject"
            app:layout_constraintTop_toTopOf="@+id/delete_subject_list_view_add_subject"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/ic_baseline_refresh_24" />

        <ImageView
            android:id="@+id/edit_subject_detail_list_view_add_attendence"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/reset_attendence_list_view_add_attendence"
            app:layout_constraintEnd_toStartOf="@+id/reset_attendence_list_view_add_attendence"
            app:layout_constraintHorizontal_bias="0.929"
            app:layout_constraintStart_toEndOf="@+id/total_classes_list_view_add_subject"
            app:layout_constraintTop_toTopOf="@+id/reset_attendence_list_view_add_attendence"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/ic_baseline_create_24" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>